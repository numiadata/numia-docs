          "mode": "NULLABLE"
        },
        {
          "name": "publish_time",
          "description": "Timestamp when the transaction was published to the mempool, indicating the time it was received by the network.",
          "type": "TIMESTAMP",
          "mode": "NULLABLE"
        },
        {
          "name": "message_id",
          "description": "Unique identifier for the transaction message, used to track its status and any potential errors.",
