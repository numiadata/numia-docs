    {
      "name": "neutron_ics_consumer_validators",
      "description": "This table provides a list of consumer validators in the Cosmos ecosystem, which are responsible for validating transactions and verifying the integrity of the blockchain. It includes information about the providers and their associated validators.",
      "tags": [
        "raw_table"
      ],
      "columns": [
        {
          "name": "provider_address",
          "description": "The address of the provider that is associated with the consumer validator.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "consumer_key_ed25519",
          "description": "The public key of the consumer validator, used for signing transactions.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "power",
          "description": "The power of the consumer validator, representing its voting weight in the network.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "rate",
          "description": "The rate at which the consumer validator is paid for its services.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "consumer_power",
          "description": "The total power of the consumer validator, including its stakes and delegations.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "consumer_commission_rate",
          "description": "The commission rate charged by the consumer validator for its services.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "provider_commission_rate",
          "description": "The commission rate charged by the provider for its services.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "moniker",
          "description": "The human-readable name of the provider or consumer validator.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "identity",
          "description": "The identity of the provider or consumer validator, which can be a public key or a name.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "website",
          "description": "The website of the provider or consumer validator.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "security_contact",
          "description": "The contact information for security-related issues with the provider or consumer validator.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "details",
          "description": "Any additional information about the provider or consumer validator.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "provider_operator_address",
          "description": "The address of the operator of the provider.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "jailed",
          "description": "A boolean indicating whether the provider or consumer validator is jailed, meaning it is unable to participate in the network.",
          "type": "BOOLEAN",
          "mode": "NULLABLE"
        },
        {
          "name": "status",
          "description": "The current status of the provider or consumer validator.",
