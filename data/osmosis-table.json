          "type": "TIMESTAMP",
          "mode": "NULLABLE"
        },
        {
          "name": "tx_hash",
          "description": "The unique hash of the transaction that created the liquidity pool position.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "tx_index",
          "description": "The index of the transaction in the block.",
          "type": "INTEGER",
          "mode": "NULLABLE"
        },
        {
          "name": "message_index",
          "description": "The index of the message (i.e., the position creation event) within the transaction.",
          "type": "INTEGER",
          "mode": "NULLABLE"
        },
        {
          "name": "action_index",
          "description": "The index of the action (i.e., the position creation action) within the message.",
          "type": "INTEGER",
          "mode": "NULLABLE"
        },
        {
          "name": "sender",
          "description": "The address of the account that created the liquidity pool position.",
          "type": "STRING",
          "mode": "NULLABLE"
        },
        {
          "name": "token_amount_0",
          "description": "The amount of the first token involved in the liquidity pool position creation.",
