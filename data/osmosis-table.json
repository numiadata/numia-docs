    {
      "name": "osmosis_ibc_transfer_delays",
      "description": "This table provides insights into the delays experienced by IBC (Inter-Blockchain Communication) transfers on the Cosmos ecosystem. It helps analyze and understand the timing discrepancies between transfer initiation and acknowledgement on various blockchains.",
      "tags": [
        "parsed_table"
      ],
      "columns": [
        {
          "name": "tx_id",
          "description": "Unique identifier of the IBC transfer transaction.",
          "type": "STRING"
        },
        {
          "name": "block_height",
          "description": "Height of the block where the IBC transfer transaction was initiated.",
          "type": "INTEGER"
        },
        {
          "name": "ack_block_height",
          "description": "Height of the block where the IBC transfer acknowledgement was received.",
          "type": "INTEGER"
        },
        {
          "name": "sender",
          "description": "Address of the account that initiated the IBC transfer transaction.",
          "type": "STRING"
        },
        {
          "name": "tx_delay",
          "description": "Time delay experienced by the IBC transfer transaction, calculated as the difference between the block timestamp and the acknowledgement block timestamp.",
          "type": "INTEGER"
        },
        {
          "name": "block_timestamp",
          "description": "Timestamp of the block where the IBC transfer transaction was initiated.",
          "type": "TIMESTAMP"
        },
        {
          "name": "ingestion_timestamp",
          "description": "Timestamp when the IBC transfer data was ingested into the database.",
          "type": "TIMESTAMP"
        }
      ]
    },
