          "type": "STRING"
        },
        {
          "name": "block_height",
          "description": "The height of the block where the transaction was initially included.",
          "type": "INTEGER"
        },
        {
          "name": "ack_block_height",
          "description": "The height of the block where the transaction was acknowledged or confirmed.",
          "type": "INTEGER"
        },
        {
          "name": "sender",
          "description": "The address of the sender who initiated the transaction.",
          "type": "STRING"
        },
        {
          "name": "tx_delay",
          "description": "The time difference between the transaction being included in a block and its acknowledgement.",
          "type": "INTEGER"
